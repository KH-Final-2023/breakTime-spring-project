<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="logImpl" value="LOG4J"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.breaktime.admin.model.vo.Notice" alias="notice"/>
    	<typeAlias type="com.kh.breaktime.businessRoom.model.vo.BusinessRoom" alias="room" />
		<typeAlias type="com.kh.breaktime.businessRoom.model.vo.BusinessRoomImg" alias="roomImg" />		
		<typeAlias type="com.kh.breaktime.member.model.vo.Member" alias="member" />
		<typeAlias type="com.kh.breaktime.business.model.vo.Business" alias="business" />
		<typeAlias type="com.kh.breaktime.review.model.vo.Review" alias="review" />
	</typeAliases>
	 
	 <mappers>
	 	<mapper resource="/mappers/notice-mapper.xml" />
	 	<mapper resource="/mappers/buRoom-mapper.xml"/>
	 	<mapper resource="/mappers/member-mapper.xml"/>
	 	<mapper resource="/mappers/business-mapper.xml"/>
	 </mappers>
  
</configuration>
